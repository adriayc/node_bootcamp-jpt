<div class="contenedor proximos-eventos">
    <h1><%= nombrePagina %></h1>

    <% if (meetis.length > 0) { %>
    <div class="grid columnas-md-3 ">
        <% meetis.forEach(meeti => { %>
            <!-- Mostra la data del JSON -->
            <%#= JSON.stringify(meeti) %>

            <!-- Incluir el partial del card -->
            <%- include('partials/card-meeti', {meeti}) %>
        <% }) %>
    </div><!-- .grid -->
    <% } else { %>
        <p>No hay meeeti's en esta categoria</p>
    <% } %>
</div>